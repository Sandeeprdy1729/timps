{
  "name": "timps",
  "version": "1.0.0",
  "description": "TIMPs - A persistent cognitive partner that remembers, evolves, and builds with its user",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node main.ts",
    "cli": "ts-node main.ts cli",
    "tui": "ts-node main.ts cli --tui",
    "server": "ts-node main.ts server",
    "init-db": "ts-node -e \"import('./db/postgres').then(m => m.initDatabase())\""
  },
  "keywords": [
    "ai",
    "agent",
    "memory",
    "llm",
    "gpt",
    "gemini",
    "ollama"
  ],
  "author": "",
