  "dependencies": {
    "@qdrant/js-client-rest": "^1.9.0",
    "blessed": "^0.1.81",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "openai": "^4.63.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.21",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "@types/pg": "^8.11.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  }
